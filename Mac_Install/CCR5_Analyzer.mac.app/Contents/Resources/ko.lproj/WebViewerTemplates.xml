<?xml version="1.0" encoding="UTF-8" ?>

<WebsiteURLTemplateList  xmlns="http://www.filemaker.com/WebsiteURLTemplate"
           xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
           xsd:noNamespaceSchemaLocation="WebViewerTemplates.xsd">

<!-- Header ends, Template list begins: -->




<!-- template supporting something like  "lhttp://www.google.co.kr/maps?q=1884+Orange+Grove+Drive,+San+Jose,+CA+95124" -->

 <WebsiteURLTemplate name="Google 지도" baseURL="http://www.google.co.kr/maps?" description="지도를 보고 위치를 찾으십시오. 미국으로 추정됩니다.">
 
 <URLElement parameterIdentifier="q" type="Virtual" virtualSeparator="," >   <!-- virtualSeparator is optional if , -->
 
  <URLVirtualParameter label="주소" userHint="필수" description="해당 위치의 주소 또는 교차로입니다(정확하지 않음)." />
  <URLVirtualParameter label="도시" userHint="필수" description="해당 위치에 있는 도시입니다(정확하지 않음)." />
  <URLVirtualParameter label="도" userHint="필수" description="해당 위치에 있는 도입니다(정확하지 않음)." />
  <URLVirtualParameter label="우편 번호" userHint="옵션" description="우편 경로 코드입니다(알고 있는 경우)." />
  <URLVirtualParameter label="국가" userHint="옵션" description="국가 위치입니다(정확하지 않음)." />
 
 </URLElement>
  
 </WebsiteURLTemplate>
 
 
 

<!-- template supporting something like "http://www.google.com/search?q=testing" -->

<WebsiteURLTemplate name="Google 웹 검색" description="웹을 검색하십시오." baseURL="http://www.google.co.kr/search?" >
 
 <URLElement parameterIdentifier="q" type="Parameter" label="쿼리" userHint="필수" description="여기에 검색 용어를 입력하십시오." />
 
</WebsiteURLTemplate>



